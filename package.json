    "export-token": "node src/trigger.js --export-token",
    "health": "node src/tools/debug.js health",
    "health-quiet": "node src/tools/debug.js health --quiet"
  },
  "keywords": [
    "antigravity",
    "ide",
    "google",
    "gemini",
    "quota",
    "automation"
  ],
  "author": "louisleonid",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonidlouis/antigravity-quota-refresher.git"
  },
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "dotenv": "^17.2.3"
  }
}