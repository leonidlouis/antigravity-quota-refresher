{
  "name": "antigravity-quota-refresher",
  "version": "1.0.0",
  "description": "Automated scheduler to align Google Antigravity IDE quota refreshes with your work hours.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
